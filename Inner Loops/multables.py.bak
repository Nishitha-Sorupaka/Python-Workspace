n=int(input("Enter the number of Inputs you want to add in List: "))
if(n<=0):
	print("Invalid Input")
else:
	lst=list()
	for i in range(1,n+1):
		val=int(input("Enter the {} value: ".format(i)))
		lst.append(val)
	else:
		print(lst)
		print("-"*30)
		n=int(input("Enter a Number:"))
		for i in range(1,11):
			print("{} X {} = {}".format(n,i,n*i))

	